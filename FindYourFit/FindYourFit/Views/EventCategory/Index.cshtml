@model List<FindYourFit.Models.EventCategory>

<h1 class="text-light">All Event Categories</h1>
<body style="background-color:darkslategrey"></body>
@if (Model.Count == 0)
{
    <p class="text-light">No scheduled events.</p>
}
else
{
    <table class="table; text-light">
        <tr>
            <th>Id
            <th>Category Name</th>
        </tr>
        @foreach (EventCategory category in Model)
        {
            <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
            </tr>
        }
    </table>
}