@model List<FindYourFit.Models.FitnessResourceCategory>
<body style="background-color:darkslategrey"></body>
<h1 class="text-light">All Fitness Resource Categories</h1>

@if (Model.Count == 0)
{
    <p class="text-light">No Fitness Resource Categories yet!</p>
}
else
{
    <table class="table; text-light">
        <tr>
            <th>Id
            <th>Category Name</th>
        </tr>
        @foreach (FitnessResourceCategory category in Model)
        {
            <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
            </tr>
        }
    </table>
}
