@model List<FindYourFit.Models.FitnessResourceCategory>

<h1>All Fitness Resource Categories</h1>

@if (Model.Count == 0)
{
    <p>No Fitness Resource Categories yet!</p>
}
else
{
    <table class="table">
        <tr>
            <th>Id
            <th>Category Name</th>
        </tr>
        @foreach (FitnessResourceCategory category in Model)
        {
            <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
            </tr>
        }
    </table>
}
